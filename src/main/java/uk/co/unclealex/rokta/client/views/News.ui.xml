<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:a="urn:import:uk.co.unclealex.rokta.client.ui">

	<g:HTMLPanel>
		<div>
			<h2>Today's games</h2>
			<a:UnorderedListPanel ui:field="todaysGamesPanel"/>
		</div>
		<div>
			<h2>The last game</h2>
			<a:UnorderedListPanel ui:field="gamePanel"/>
		</div>
		<div>
			<h2>Current streaks</h2>
			<a:UnorderedListPanel ui:field="currentStreaksPanel"/>
		</div>
		<div>
			<h2>The league</h2>
			<a:UnorderedListPanel ui:field="leaguePanel"/>
		</div>
	</g:HTMLPanel>

</ui:UiBinder>