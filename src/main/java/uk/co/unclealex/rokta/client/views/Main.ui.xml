<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:style>
		.navExtension {
			position: absolute;
			left: 0px;
			top: 0px;
			width: 180px;
			height: 58px;
			background: #eeeeee;
			z-index: -1;
	  }
	</ui:style>
	<g:DockLayoutPanel unit="PX">
		<g:north size="58">
			<g:HTMLPanel>
				<div class="{style.navExtension}"/>
				<g:SimplePanel ui:field="authenticationPanel"/>
				<g:SimplePanel ui:field="titlePanel"/>
			</g:HTMLPanel>
		</g:north>
		<g:west size="180">
		  <g:SimpleLayoutPanel ui:field="navigationPanel"/>		
		</g:west>
		<g:center>
			<g:SimpleLayoutPanel ui:field="mainPanel"/>
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder>