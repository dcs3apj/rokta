<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<g:DockLayoutPanel unit="PX">
		<g:north size="50">
			<g:HTMLPanel>
				<g:SimplePanel ui:field="authenticationPanel"/>
				<g:SimplePanel ui:field="titlePanel"/>
			</g:HTMLPanel>
		</g:north>
		<g:west size="180">
		  <g:SimpleLayoutPanel ui:field="navigationPanel"/>		
		</g:west>
		<g:center>
			<g:SimpleLayoutPanel ui:field="mainPanel"/>
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder>