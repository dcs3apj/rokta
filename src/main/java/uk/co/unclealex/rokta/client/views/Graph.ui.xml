<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:a="urn:import:uk.co.unclealex.rokta.client.ui">
	
	<ui:style>
		.container {
	    width: 98%;
	    height: 98%;
	    margin-top: 2%;
	    margin-bottom: 2%;
	    margin-right: 2%;	    
		}
		.graph {
	    width: 100%;
	    height: 100%;
		}
	</ui:style>
	<g:HTMLPanel addStyleNames="{style.container}">
		<g:SimplePanel ui:field="graph" addStyleNames="{style.graph}"/>
	</g:HTMLPanel>
</ui:UiBinder>